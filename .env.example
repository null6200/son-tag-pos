# ===========================================
# SonTag POS - Environment Configuration
# ===========================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!

# PostgreSQL Database
POSTGRES_USER=pos_user
POSTGRES_PASSWORD=CHANGE_ME_USE_STRONG_PASSWORD_HERE

# Redis Cache
REDIS_PASSWORD=CHANGE_ME_USE_STRONG_PASSWORD_HERE

# JWT Authentication (generate with: openssl rand -base64 64)
JWT_SECRET=CHANGE_ME_USE_64_CHAR_RANDOM_STRING_HERE

# CORS Allowed Origins (comma-separated)
ALLOWED_ORIGINS=https://stanfordelaze.com,https://your-frontend-domain.com

# Optional: Sentry Error Tracking
# SENTRY_DSN=https://your-sentry-dsn
# SENTRY_TRACES_SAMPLE_RATE=0.1
